{{#each posts as |post| }}
<div>
   <div class="card text-white bg-secondary" >
  <h2 class="card-header">{{post.title}}</h2>
  <div class="card-body ">
    <p class="card-text">{{post.content}}</p>
  </div>
  <div class="card-body ">
    <p class="card-text">{{post.user_id}}</p>
  </div>
  <div class="card-body ">
    <p class="card-text">{{post.date_created}}</p>
  </div>
  <div class="card-body ">
    <p class="card-text">{{post.comments}}</p>
  </div>
</div>
  <br>
{{/each}}
